@model IEnumerable<MemberProfileModel>

@{
    Layout = "_MainLayout";
    ViewData["Title"] = "Members";
}


<section id="members">
    <header>
        <div class="header-container">
            <h2>@ViewData["Title"]</h2>
            @await Html.PartialAsync("Components/Buttons/_AddButton", "Add Member")
        </div>
    </header>

    <div class="member-list">
        @if (Model != null)
        {
            @foreach (var member in Model)
            {
                <partial name="Sections/Members/_MemberCard" model="member" />
            }
        }
    </div>
</section>